# 7. Write a shell script which displays January if we enter Jan, Janu, Janua or January

echo "Enter a month:"
read month
case $month in
	"Jan")
	echo "January" ;;
		
	"Janu")
	echo "January" ;;
		
	"Janua")
	echo "January" ;;
		
	"January")
	echo "January" ;;
		
	*)
	echo "Can not understand" ;;
		
esac #To end the case 

